  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "build": "tsc",
    "build:watch": "onchange 'src/**/*.ts' -- npm run build",
    "view:coverage": "serve coverage/lcov-report",
    "clean": "rm -rf coverage src/**/*.js src/**/*.map",
    "format": "prettier --write \"src/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chess-Centre/chess-players.git"
  },
  "keywords": [
    "chess",
    "players",
    "nodejs"
  ],
  "author": "matt-d-webb <matt.d.webb@icloud.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Chess-Centre/chess-players/issues"
  },
  "homepage": "https://github.com/Chess-Centre/chess-players#readme",
  "devDependencies": {
    "@types/adm-zip": "^0.4.33",
    "@types/axios": "^0.14.0",
    "@types/cli-table": "^0.3.0",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@types/source-map-support": "^0.5.3",
    "coveralls": "^3.1.0",
    "jest": "^29.7.0",
    "onchange": "^7.0.2",
    "prettier": "^2.1.1",
    "serve": "^14.1.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^29.2.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.21.0",
    "adm-zip": "^0.4.16",
    "axios": "^1.7.9",
    "cli-progress": "^3.12.0",
    "decko": "^1.2.0",
    "fast-xml-parser": "^4.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
